{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "@api_url",
        "NEXT_PUBLIC_AGORA_APP_ID": "@agora_app_id"
    }
}